<mui:ModernWindow x:Class="RTI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="M.A.M.E" IsTitleVisible="True"
        ContentSource="/Pages/home.xaml" Closed="ModernWindow_Closed" Loaded="ModernWindow_Loaded">
    <!-- Icon="/rediconpng.png" -->


    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="welcome">
            <mui:LinkGroup.Links>
                
                
                <mui:Link DisplayName="Home" Source="/Pages/home.xaml" />
           
                
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup DisplayName="settings" GroupKey="settings">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="software" Source="/Pages/SettingsPage.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>

    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="settings" Source="/Pages/SettingsPage.xaml" />
        <mui:Link DisplayName="help" Source="/Pages/SettingsPage.xaml" />
    </mui:ModernWindow.TitleLinks>


</mui:ModernWindow>
